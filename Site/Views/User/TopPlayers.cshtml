@using Site.Data.Models;
@using Site.Data.Models.User;
@model Identified<IDictionary<long,UserInfo>>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>title</title>
</head>
<body>
@{
    foreach (var (pos,user) in Model.Value)
    {
        <h2>@pos</h2>
        @await Html.PartialAsync("UserInfo", new Identified<UserInfo>(Model.Credential,user))
    }
}
</body>
</html>